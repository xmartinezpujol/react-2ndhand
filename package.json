      ".*": "./node_modules/babel-jest"
    },
    "setupFiles": [
      "./jestsetup.js"
    ],
    "modulePathIgnorePatterns": [
      "cypress"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    }
  },
  "devDependencies": {
