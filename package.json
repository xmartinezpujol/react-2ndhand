{
  "name": "react-2ndhand",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "babel-eslint": "10.0.1",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-redux": "6.0.1",
    "react-router": "4.3.1",
